<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.chensolo.liistview">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".DataPicker_TimePicker"></activity>
        <activity android:name=".GridViewActivity"></activity>
        <activity android:name=".Spinner"></activity>
        <activity android:name=".ListViewActivity"></activity>
        <activity android:name=".ProgressBar"></activity>
        <activity android:name=".WebviewActivity"></activity>
        <activity android:name="com.example.fragment.FragmentActivity_two"></activity>
        <activity android:name="com.example.fragment.FragmentActivity"></activity>
        <activity android:name="com.example.fragment.FragmentActivity_three"></activity>
      <activity android:name="com.example.fragment.FragmentActivity_four"></activity>
    <activity android:name="com.example.ViewPager.ViewPagerActivity"></activity>
     </application>

</manifest>